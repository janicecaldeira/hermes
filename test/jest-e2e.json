{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "runner": "jest-serial-runner",
  "setupFiles": [
    "../src/factories/index.ts"
  ],
  "setupFilesAfterEnv": [
    "jest-extended/all"
  ],
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "^@/(.*)$": "<rootDir>/../src/$1"
  }
}